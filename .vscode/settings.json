{
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true,
            "source.fixAll": true
        },

    },
    "isort.args": [
        "--profile",
        "black"
    ],
    "cSpell.words": [
        "structuralize"
    ],

}